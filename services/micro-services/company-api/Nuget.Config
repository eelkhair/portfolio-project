<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
        <add key="local" value="https://nuget.eelkhair.net/v3/index.json" />
    </packageSources>
    <packageSourceMapping>
        <packageSource key="local">
            <package pattern="JobBoard.*" />
            <package pattern="Elkhair.*" />
        </packageSource>
        <packageSource key="nuget.org">
            <package pattern="*" />
        </packageSource>
    </packageSourceMapping>
</configuration>