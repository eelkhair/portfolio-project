{
  "$schema": "http://json.schemastore.org/launchsettings.json",
 
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:37553",
      "sslPort": 44341
    }
  },
  "profiles": { 
    "dapr": {
      "commandName": "Executable",
      "workingDirectory": "$(ProjectDir)",
      "launchBrowser": true,
      "applicationUrl": "http://localhost:5282",
      "executablePath": "dapr.exe",
      "commandLineArgs": "run --app-id user-api --resources-path ../Components/ --resources-path ./Dapr/Components/Secrets --resources-path ./Dapr/Components/Timers  --config ../Config/config.yaml --app-port 5292 --dapr-http-port 6095  --log-level debug dotnet run"
    },
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "http://localhost:5292",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "https://localhost:7021;http://localhost:5292",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
